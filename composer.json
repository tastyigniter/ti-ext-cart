{
  "name": "tastyigniter/ti-ext-cart",
  "type": "tastyigniter-extension",
  "description": "Easily add a shopping cart to your site. The most powerful way to sell your menu items.",
  "keywords": [
    "tastyigniter",
    "cart",
    "online-food-ordering",
    "ordering"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sam Poyigi",
      "email": "sam@sampoyigi.com"
    }
  ],
  "require": {
    "tastyigniter/flame": "v4.x-dev",
    "tastyigniter/ti-ext-local": "v4.x-dev",
    "tastyigniter/ti-ext-payregister": "v4.x-dev",
    "tastyigniter/ti-ext-user": "v4.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Igniter\\Cart\\": "src/"
    }
  },
  "extra": {
    "tastyigniter-extension": {
      "name": "Cart",
      "icon": {
        "class": "fa fa-shopping-cart",
        "color": "#FFF",
        "backgroundColor": "#ED561A"
      },
      "homepage": "https://tastyigniter.com/marketplace/item/igniter-cart"
    }
  }
}
